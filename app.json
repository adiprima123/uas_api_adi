{
  "expo": {
    "name": "MovieFinder",
    "slug": "uas-api",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "uasapi",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f0f"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#0f0f0f",
          "preventAutoHide": true
        }
      ],
      "expo-router"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0f0f0f"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.me.marsx.uasapi"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.me.marsx.uasapi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30541bf6-e629-4eca-9502-3f29518f0437"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
